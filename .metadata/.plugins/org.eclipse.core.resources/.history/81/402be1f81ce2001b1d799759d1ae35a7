package day1;


public class App {
		
		
		static void scenario1()
		{
			Student student=new Student();
			student.setId(101);
			student.setName("Ram");
			student.setDob("12-01;1999");
			student.setCollegename="VIIT ";
			display in=new display();
			in.display(student);
			Student stud=new Student(102, "Utkarsh", "23-01-1999");
			in.display(stud;
			
		}
		
		
		static void scenario2()
		{
			Student stu[]=new Student[3];
			stu[0]=new Student();
			stu[0].setId(201);
			stu[0].setName("Rajesh");
			stu[0].setDob("01-01-1990");
			display in=new display();
			stu[1]=new Student(202,"Suraj","23-11-1997");
			stu[2]=new Student();
			stu[2].setId(203);
			stu[2].setName("Ahmed");
			stu[2].setDob("28-07-2000");
			for(Student x:stu) {
				in.display(x);
			}
		}
	        
	        public static void main(String[] args){
				scenario1();
				scenario2();
				//Scenario3();
			}
		}
		
}
